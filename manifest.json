{
  "manifest_version": 2,

  "name": "YTStats",
  "description": "Keep track of YouTube viewing stats",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "YTStats"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
